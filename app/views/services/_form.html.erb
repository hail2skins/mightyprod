<%= simple_form_for([@business, @service]) do |f| %>
  <%= f.error_notification %>
  
  <%= f.input :name, label: "Service name" %>
  <%= f.input :description %>
  <%= f.simple_fields_for :prices do |price| %>
    <%= render partial: 'prices/form', locals: { f: price } %>
  <% end %>
  
  <%= f.button :submit %>

<% end %>
