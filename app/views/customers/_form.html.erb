<%= simple_form_for([@business, @customer]) do |f| %>
  <%= f.error_notification %>
  
  <%= f.input :first_name %>
  <%= f.input :middle_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :referred_by %>
  



    <%= f.simple_fields_for :phones do |phones| %>
        <%= render partial: 'phones/form', locals: { f: phones } %>
    <% end %>


  <%= f.button :submit %>
<% end %>